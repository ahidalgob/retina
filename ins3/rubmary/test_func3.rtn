# Probar que no pueda usarse un tipo de retorno distinto al declarado.
func plus(number op1, number op2) -> boolean
begin
    return op1 + op2;
end;
program
  with
      number i;
      number j = 78 + plus(4, 3);
  do
      i = plus(3,2);
        with
          boolean i;
        do
          i = true;
        end;
      j = 78 + i;
  end;
end;